{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get"
      ],
      "route": "v1/library/categories/{route:regex(\\w+)?}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}